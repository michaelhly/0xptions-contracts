    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "download-cli": "^1.1.1",
    "ganache-cli": "^6.2.5",
    "openzeppelin-solidity": "2.0.0",
    "stringify-object": "^3.3.0",
    "truffle": "4.1.13",
    "truffle-hdwallet-provider": "^1.0.2",
    "veil-contracts": "https://github.com/veilco/veil-contracts",
    "web3": "^1.0.0-beta.37"
  }
